{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python 디버거: main_imagenet.py 실행",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main_imagenet.py",
            "args": [
                "--data_path",
                "data/ImageNet",
                "--arch",
                "resnet18",
                "--n_bits_w",
                "4",
                "--n_bits_a",
                "4",
                "--weight",
                "0.01",
                "--T",
                "4.0",
                "--lamb_c",
                "0.02"
            ],
            "console": "integratedTerminal",
            "stopOnEntry": true,
            "justMyCode": true,
        }
    ]
}